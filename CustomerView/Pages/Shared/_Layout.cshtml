<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Luxury Diamond Website">
    <link rel="stylesheet" href="~/css/site.css">
    <title>@ViewData["Title"] - Luxury Diamond</title>
</head>
<body>
    <div>
        @await Html.PartialAsync("_Header")
    </div>
    <div class="container">
        @RenderBody()
    </div>
    <div>
        @await Html.PartialAsync("_Footer")
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>
